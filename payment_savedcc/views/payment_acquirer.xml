<?xml version="1.0" encoding="utf-8"?><openerp>    <data>        <record id="transaction_form_savedcc" model="ir.ui.view">            <field name="name">acquirer.transaction.form.moneris</field>            <field name="model">payment.transaction</field>            <field name="inherit_id" ref="payment.transaction_form"/>            <field name="arch" type="xml">                <xpath expr='//notebook' position='inside'>                    <page string="Saved CC TX Details">                        <group>                            <field name="savedcc_name"/>                            <field name="savedcc_type"/>                            <field name="savedcc_number"/>                            <field name="savedcc_expdate"/>                            <field name="savedcc_code"/>                        </group>                    </page>                </xpath>            </field>        </record>    </data></openerp>